;AAsBA,EAAA,OAAA,EAAA;AACA,IAAA,WADA,uBACA,KADA,EACA;AACA,WAAA,IAAA,GAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA,KAHA;AAIA,IAAA,UAJA,sBAIA,IAJA,EAIA;AACA,WAAA,SAAA,GAAA,EAAA;AACA,WAAA,QAAA,GAAA,IAAA;AACA,KAPA;AAQA,IAAA,QARA,oBAQA,IARA,EAQA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,gBAAA,IAAA,CAAA,GAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,QAAA,CAAA,KADA;AAEA,kBAAA,WAAA,EAAA,MAAA,CAAA,QAAA,CAAA;AAFA,iBAAA,CAFA;;AAAA;AAMA,gBAAA,MAAA,CAAA,QAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AAPA,iDAQA,IARA;;AAAA;AAAA;AAAA;AAUA,gBAAA,OAAA,CAAA,GAAA;;AAVA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA,KApBA;AAqBA,IAAA,UArBA,sBAqBA,IArBA,EAqBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,MAAA,CAAA,gBAAA,IAAA,CAAA,GAAA,CAFA;;AAAA;AAGA,gBAAA,MAAA,CAAA,QAAA,GAAA,IAAA;;AACA,gBAAA,MAAA,CAAA,QAAA;;AAJA,kDAKA,IALA;;AAAA;AAAA;AAAA;AAOA,gBAAA,OAAA,CAAA,GAAA;;AAPA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KA9BA;AA+BA,IAAA,MA/BA,oBA+BA;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA,qBAEA,MAAA,CAAA,IAFA;AAAA;AAAA;AAAA;;AAGA,gBAAA,QAHA,GAGA,IAAA,QAAA,EAHA;AAIA,gBAAA,QAAA,CAAA,MAAA,CAAA,OAAA,EAAA,MAAA,CAAA,IAAA,EAAA,MAAA,CAAA,IAAA,CAAA,IAAA;AAJA;AAAA,uBAKA,KAAA,CAAA,IAAA,CAAA,aAAA,EAAA,QAAA,CALA;;AAAA;AAKA,gBAAA,EALA;AAAA;AAAA,uBAMA,KAAA,CAAA,IAAA,CAAA,YAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,KADA;AAEA,kBAAA,IAAA,EAAA,EAAA,CAAA,IAAA,CAAA,IAFA;AAGA,kBAAA,WAAA,EAAA,MAAA,CAAA;AAHA,iBAAA,CANA;;AAAA;AAMA,gBAAA,EANA;AAWA,gBAAA,MAAA,CAAA,OAAA,GAAA,EAAA,CAAA,IAAA;AAXA;AAAA;;AAAA;AAAA;AAAA,uBAcA,KAAA,CAAA,IAAA,CAAA,YAAA,EAAA;AACA,kBAAA,KAAA,EAAA,MAAA,CAAA,KADA;AAEA,kBAAA,IAAA,EAAA,IAFA;AAGA,kBAAA,WAAA,EAAA,MAAA,CAAA;AAHA,iBAAA,CAdA;;AAAA;AAcA,gBAAA,EAdA;AAmBA,gBAAA,MAAA,CAAA,OAAA,GAAA,EAAA,CAAA,IAAA;;AAnBA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAsBA,gBAAA,OAAA,CAAA,GAAA;;AAtBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAwBA,KAvDA;AAwDA,IAAA,QAxDA,sBAwDA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA,GAAA,CAAA,YAAA,CAFA;;AAAA;AAEA,gBAAA,QAFA;AAGA,gBAAA,MAAA,CAAA,KAAA,GAAA,QAAA,CAAA,IAAA;AAHA,kDAIA,IAJA;;AAAA;AAAA;AAAA;AAMA,gBAAA,OAAA,CAAA,GAAA;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA;AAhEA;AAtBA,CAAA","sourcesContent":["<template>\n<div class=\"page-content\">\n  <div class=\"admin\">\n    <div id=\"newpost\"><h1>New Post</h1></div>\n    <div class='postbox'>\n      <div class=\"heading\">\n        <div class=\"circle\">+</div>\n        <h2>Write New Post</h2>\n      </div>\n      <div class=\"add\">\n        <div class=\"form\">\n          <input id=\"long\" v-model=\"title\" placeholder=\"Add Title\">\n          <textarea v-model=\"description\" placeholder=\"Write Post\"></textarea>\n          <input type=\"file\" name=\"photo\" @change=\"fileChanged\">\n          <button @click=\"upload\">Upload</button>\n        </div>\n        <div class=\"upload\" v-if=\"addItem\">\n          <h2>{{addItem.title}}</h2>\n          <h3>{{addItem.description}}</h3>\n          <img :src=\"addItem.path\" />\n        </div>\n      </div>\n      <div class=\"heading\">\n        <div class=\"circle\">-</div>\n        <h2>Edit/Delete a Post</h2>\n      </div>\n      <div class=\"edit\">\n        <div class=\"form\">\n          <input v-model=\"findTitle\" placeholder=\"Search\">\n          <div class=\"suggestions\" v-if=\"suggestions.length > 0\">\n            <div class=\"suggestion\" v-for=\"s in suggestions\" :key=\"s.id\" @click=\"selectItem(s)\">{{s.title}}\n            </div>\n          </div>\n        </div>\n        <div class=\"upload\" v-if=\"findItem\">\n          <input v-model=\"findItem.title\">\n          <input v-model=\"findItem.description\">\n          <p></p>\n          <img :src=\"findItem.path\" />\n        </div>\n        <div class=\"actions\" v-if=\"findItem\">\n          <button @click=\"deleteItem(findItem)\">Delete</button>\n          <button @click=\"editItem(findItem)\">Edit</button>\n        </div>\n      </div>\n    </div>\n  </div>\n</div>\n</template>\n\n<script>\nimport axios from 'axios';\nexport default {\n  name: 'Admin',\n  data() {\n    return {\n      title: \"\",\n      file: null,\n      addItem: null,\n      description: \"\",\n      items: [],\n      findTitle: '',\n      findItem: null,\n    }\n  },\n  computed: {\n    suggestions() {\n      let items = this.items.filter(item => item.title.toLowerCase().startsWith(this.findTitle.toLowerCase()));\n      return items.sort((a, b) => a.title > b.title);\n    }\n  },\n  created() {\n    this.getItems();\n  },\n  methods: {\n    fileChanged(event) {\n      this.file = event.target.files[0]\n    },\n    selectItem(item) {\n      this.findTitle = \"\";\n      this.findItem = item;\n    },\n    async editItem(item) {\n      try {\n        await axios.put(\"/api/items/\" + item._id, {\n          title: this.findItem.title,\n          description: this.findItem.description,\n        });\n        this.findItem = null;\n        this.getItems();\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async deleteItem(item) {\n      try {\n        await axios.delete(\"/api/items/\" + item._id);\n        this.findItem = null;\n        this.getItems();\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async upload() {\n      try {\n        if (this.file)  {\n        const formData = new FormData();\n        formData.append('photo', this.file, this.file.name)\n        let r1 = await axios.post('/api/photos', formData);\n        let r2 = await axios.post('/api/items', {\n          title: this.title,\n          path: r1.data.path,\n          description: this.description,\n        });\n        this.addItem = r2.data;\n        }\n        else {\n        let r2 = await axios.post('/api/items', {\n          title: this.title,\n          path: null,\n          description: this.description,\n        });\n        this.addItem = r2.data;\n        }\n      } catch (error) {\n        console.log(error);\n      }\n    },\n    async getItems() {\n      try {\n        let response = await axios.get(\"/api/items\");\n        this.items = response.data;\n        return true;\n      } catch (error) {\n        console.log(error);\n      }\n    },\n  }\n}\n</script>\n\n<style scoped>\n.image h2 {\n  font-style: italic;\n  font-size: 1em;\n}\n.image h2 {\n  font-size: .5em;\n}\n\n.page-content {\n  align-items: center;\n\n  width: auto;\n}\n\n.admin {\n  margin-left: 33.3%;\n  width: 33.3%;\n  height: auto;\n\n}\n\n#newpost {\n  text-align: center;\n  height: auto;\n}\n\n.postbox {\n  width: auto;\n}\n\n#long {\n  padding-right: 200px;\n  padding-bottom: 30px;\n}\n\ntextarea {\n  resize: none;\n  height: 400px;\n  width: 400px;\n}\n\nh1 {\n  font-size: 30px;\n}\n\n.heading {\n  display: flex;\n  margin-bottom: 20px;\n  margin-top: 20px;\n}\n\n.heading h2 {\n  margin-top: 8px;\n  margin-left: 10px;\n}\n\n.add,\n.edit {\n  display: flex;\n}\n\n.circle {\n  width: 18px;\n  height: 18px;\n  padding: 5px;\n  background: white;\n  border: 1px dotted grey;\n  color: dark grey;\n  font-size: 15px;\n  text-align: center\n}\n\n/* Form */\ninput,\ntextarea,\nselect,\nbutton {\n  font-family: 'Montserrat', sans-serif;\n  font-size: 1em;\n}\n\n.form {\n  margin-right: 50px;\n}\n\n/* Uploaded images */\n.upload h2 {\n  margin: 0px;\n}\n\n.upload img {\n  max-width: 300px;\n  height: auto;\n}\n\n/* Suggestions */\n.suggestions {\n  width: 200px;\n  border: 1px solid #ccc;\n}\n\n.suggestion {\n  min-height: 20px;\n}\n\n.suggestion:hover {\n  background-color: #5BDEFF;\n  color: #fff;\n}\n\nh2 {\n  font-size: 14px;\n}\n</style>\n"],"sourceRoot":"src/views"}]}